#!/bin/bash
#SBATCH --partition=experimental
#SBATCH --nodes=9
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=1
#SBATCH --ntasks-per-core=1
#SBATCH --gres=gpu:2
#SBATCH --mem=24000
#SBATCH --time=00:30:00
#SBATCH --output=slurm_9_1_1_a.out_
export OMP_NUM_THREADS=1
export KMP_NUM_THREADS=1
export MV2_ENABLE_AFFINITY=0
# - comment stride = 24
. /etc/profile.d/modules.bash
module load mvapich2/1.7
module load cuda

mpiexec.hydra -rmk slurm ./a.out     

